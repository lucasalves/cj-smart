tocTab[tocTab.length] = new Array("2.3.1.1.1.2.3:1", "", "Regras de Negócio", "EARoot/EA2/EA3/EA1/EA1/EA1/EA2/EA3/EA158.htm", "188.png", "130", "0", "", "Custom diagram0","{943B7B30-3DA7-4073-AA0E-7666BBB14738}");
tocTab[tocTab.length] = new Array("2.3.1.1.1.2.3:2", "", "«Functional» R006 - O sistema DEVE avisar a coordenadora de ensino quando um Aluno tiver de ser advertido informando a razão da advertência, conforme as regras: RN012, RN013.<br />", "EARoot/EA2/EA3/EA1/EA1/EA1/EA2/EA3/EA160.htm", "19.png", "1876", "0", "", "Requirement0","{0F98E0C5-55CC-46ac-A979-B8BDC818B329}");
tocTab[tocTab.length] = new Array("2.3.1.1.1.2.3:3", "", "«Functional» RN010 - Um aluno pode apresentar atestados para abonar faltas, este atestado tem de ser validade pela secretária para que o abono tenha efeito.", "EARoot/EA2/EA3/EA1/EA1/EA1/EA2/EA3/EA161.htm", "19.png", "727", "0", "", "Requirement0","{0F64E3F1-C605-4530-8497-D3BFFA264F03}");
tocTab[tocTab.length] = new Array("2.3.1.1.1.2.3:4", "", "«Functional» RN011 - Após validado o atestado pode ser efetuado o abono das faltas referentes aos dias qua constam do atestado.", "EARoot/EA2/EA3/EA1/EA1/EA1/EA2/EA3/EA162.htm", "19.png", "728", "0", "", "Requirement0","{C62DA0EB-399A-497b-8E9C-1984653F4CF9}");
tocTab[tocTab.length] = new Array("2.3.1.1.1.2.3:5", "", "«Functional» RN012 - Quando um aluno recebe três ocorrências automaticemente ele recebe uma advertência.<br />", "EARoot/EA2/EA3/EA1/EA1/EA1/EA2/EA3/EA163.htm", "19.png", "729", "0", "", "Requirement0","{16BFF42D-5470-499b-82F8-B31AE6968F09}");
tocTab[tocTab.length] = new Array("2.3.1.1.1.2.3:6", "", "«Functional» RN013 - Um aluno  pode ser advertido em função de uma ocorrência grave mesmo que não tenha recebido ocorrência anteriores.", "EARoot/EA2/EA3/EA1/EA1/EA1/EA2/EA3/EA164.htm", "19.png", "1875", "0", "", "Requirement0","{A193C103-26A2-4828-A309-CF48F5439E01}");
tocTab[tocTab.length] = new Array("2.3.1.1.1.2.3:7", "", "«Functional» RN015 - Quando um aluno é advertido o fato deve ser comunicado ao responsável pelo aluno.13<br />", "EARoot/EA2/EA3/EA1/EA1/EA1/EA2/EA3/EA165.htm", "19.png", "733", "0", "", "Requirement0","{B9BB311B-B340-4de7-8E05-D0586A187C93}");
tocTab[tocTab.length] = new Array("2.3.1.1.1.2.3:8", "", "«Functional» RN016 - O Educador receberá no início de cada semestre um diário de aula para que anote faltas, notas e ocorrências. O educador devera registrar diariamente as faltas e ocorrências e mensalmente as notas. Deve ser cobrado caso isto não aconteça. 14<br />", "EARoot/EA2/EA3/EA1/EA1/EA1/EA2/EA3/EA166.htm", "19.png", "736", "0", "", "Requirement0","{72460BDC-3C15-4f7f-AC1B-80F1D7AE9132}");
tocTab[tocTab.length] = new Array("2.3.1.1.1.2.3:9", "", "«Functional» RN017 -  Um boletim mensal pode ser solicitado pelo aluno a qualquer momento, no qual consta as notas do mês corrente  média das notas. 15<br />", "EARoot/EA2/EA3/EA1/EA1/EA1/EA2/EA3/EA167.htm", "19.png", "741", "0", "", "Requirement0","{B75ADCEC-A0B4-4eb1-B437-1E690D30FBC6}");
tocTab[tocTab.length] = new Array("2.3.1.1.1.2.3:10", "", "«Functional» RN018 -  Um boletim semestral pode ser solicitado pelo aluno a qualquer momento, no qual consta as notas do mês corrente e dos meses anteriores com a média das notas. 16<br />", "EARoot/EA2/EA3/EA1/EA1/EA1/EA2/EA3/EA168.htm", "19.png", "743", "0", "", "Requirement0","{CF7C2A5D-ACB6-46a0-B2EE-AE64BDBCE515}");
tocTab[tocTab.length] = new Array("2.3.1.1.1.2.3:11", "", "«Functional» RN019 - Ao fim do curso o semestre é finalizado e são gerados certificados para os alunos aprovados. Um Aluno é aprovado caso consiga a média minima estabelecida em cada uma das matérias e não exceda o número de faltas permitidas para cada curso. 17<br />", "EARoot/EA2/EA3/EA1/EA1/EA1/EA2/EA3/EA169.htm", "19.png", "745", "0", "", "Requirement0","{F6DC69F6-E7E9-49be-AEF6-32B596FCD504}");
tocTab[tocTab.length] = new Array("2.3.1.1.1.2.3:12", "", "«Functional» RN020 - Caso o tenha duas faltas num intervalo de menos de cinco dias, sem abono, os responsáveis devem ser comunicados deste fato. 18<br />", "EARoot/EA2/EA3/EA1/EA1/EA1/EA2/EA3/EA170.htm", "19.png", "747", "0", "", "Requirement0","{972DE9F8-0CB6-4ed8-B4A8-552EF5341F95}");
tocTab[tocTab.length] = new Array("2.3.1.1.1.2.3:13", "", "«Functional» RN021 -  Todo aluno deve ter acesso ao calendário de atividades semestrais de sua turma. 19<br />", "EARoot/EA2/EA3/EA1/EA1/EA1/EA2/EA3/EA171.htm", "19.png", "774", "0", "", "Requirement0","{AC00C9A0-9C88-478f-8B0D-C4CDA0EC658F}");
